@model IEnumerable<SpotifyWPFSearch.Models.Item>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.disc_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.duration_ms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@explicit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.href)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.is_local)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.popularity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.preview_url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.track_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.uri)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.disc_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.duration_ms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.@explicit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.href)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.is_local)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.popularity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.preview_url)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.track_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.uri)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
